<project version="2.0">
  <pkg-config>
    <pkg tool="perl">default</pkg>
  </pkg-config>
  <syspaths>
    <path>../libzeep</path>
    <path>../clustal-omega/include/clustalo</path>
  </syspaths>
  <userpaths>
    <path>src</path>
    <path>../mrs/lib/Sources</path>
  </userpaths>
  <libpaths>
    <path>../mrs/lib</path>
    <path>../libzeep</path>
    <path>/usr/local/lib</path>
    <path>../clustal-omega/lib</path>
  </libpaths>
  <files>
    <group name="Sources">
      <file>matrix.cpp</file>
      <file>primitives-3d.cpp</file>
      <file>structure.cpp</file>
      <file>utils.cpp</file>
      <file>dssp.cpp</file>
      <file>mkhssp.cpp</file>
    </group>
    <group name="Libraries">
      <link>mrsd</link>
      <link>boost_date_time</link>
      <link>boost_filesystem</link>
      <link>boost_iostreams</link>
      <link>boost_program_options</link>
      <link>boost_system</link>
      <link>boost_thread</link>
      <link>boost_regex</link>
      <link>bz2</link>
      <link>z</link>
      <link>zeep</link>
      <link>clustalo</link>
    </group>
  </files>
  <targets>
    <target kind="Executable">
      <name>hssp-Debug</name>
      <linkTarget>hssp-2</linkTarget>
      <compiler>/usr/bin/g++</compiler>
      <defines>
        <define>DEBUG</define>
        <define>LINUX</define>
        <define>USE_COMPRESSION</define>
      </defines>
      <cflags>
        <cflag>-fopenmp</cflag>
        <cflag>-O0</cflag>
        <cflag>-pthread</cflag>
        <cflag>-gdwarf-2</cflag>
        <cflag>-std=c++0x</cflag>
      </cflags>
      <warnings>
        <warning>all</warning>
        <warning>no-multichar</warning>
      </warnings>
    </target>
    <target kind="Executable">
      <name>hssp-Release</name>
      <linkTarget>hssp-2</linkTarget>
      <compiler>/usr/bin/g++</compiler>
      <defines>
        <define>NDEBUG</define>
        <define>LINUX</define>
        <define>USE_COMPRESSION</define>
      </defines>
      <cflags>
        <cflag>-O3</cflag>
        <cflag>-pthread</cflag>
        <cflag>-fopenmp</cflag>
        <cflag>-std=c++0x</cflag>
      </cflags>
      <warnings>
        <warning>all</warning>
        <warning>no-multichar</warning>
      </warnings>
    </target>
  </targets>
</project>
